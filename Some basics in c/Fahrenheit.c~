#include <stdio.h>

int main() {

    float celsius, fahr;
    int step, upper, lower;

    step = 20;
    lower = 0;
    upper = 300;

    fahr = lower;

    while(fahr <= upper) {
	celsius = (5.0/9.0) * (fahr - 32);
	printf("(%3.0f)---(%6.2f)\n", fahr, celsius);
        fahr = fahr + step;
    }
}
    
